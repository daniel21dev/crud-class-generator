{
  "name": "crud-class-generator",
  "description": "generates a class with basic crud methods in base of prisma datamodel",
  "version": "1.0.3",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "crud-class-generator": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "3.15.2",
    "@prisma/generator-helper": "3.12.0",
    "@prisma/sdk": "3.12.0",
    "prettier": "2.5.1"
  },
  "devDependencies": {
    "@types/jest": "29.2.3",
    "@types/node": "17.0.21",
    "@types/prettier": "2.4.2",
    "jest": "29.3.1",
    "prisma": "3.12.0",
    "ts-jest": "27.1.4",
    "typescript": "4.6.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^18.0.1"
  },
  "homepage": "https://github.com/daniel21dev/crud-class-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel21dev/crud-class-generator"
  },
  "author": "daniel romero",
  "keywords": [
    "prisma",
    "prisma-crud",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
